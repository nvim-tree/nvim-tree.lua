{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime.version": "Lua 5.1",
  "workspace": {
    "library": [
      "$VIMRUNTIME/lua/vim/_meta",
      "$VIMRUNTIME/lua/vim/lsp",
      "${3rd}/luv/library"
    ]
  },
  "diagnostics": {
    "globals": [
      "vim"
    ],
    "disable": [
      "deprecated",
      "undefined-field"
    ]
  }
}
