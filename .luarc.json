{
	"$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
	"runtime.version" : "Lua 5.1",
	"diagnostics": {
		"globals": [
			"vim"
		],
		"disable": [
			"cast-local-type",
			"lowercase-global",
			"missing-parameter",
			"missing-return",
			"missing-return-value",
			"need-check-nil",
			"param-type-mismatch"
		]
	}
}

